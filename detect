if game.PlaceId == 4779613061 then -- sl2
  loadstring(game:HttpGet('https://scripts.luawl.com/10812/SouthLondon.lua'))()
elseif game.PlaceId == 2788229376 then -- dh
  loadstring(game:HttpGet('https://scripts.luawl.com/10813/DaHood.lua'))()
elseif game.PlaceId == 7167319176 then --Chicago-Remastered
  loadstring(game:HttpGet('https://scripts.luawl.com/10815/ChicagoRemastered.lua'))()
elseif game.PlaceId == 9796315265 then --Streetz War
    loadstring(game:HttpGet('https://scripts.luawl.com/11805/StreetzWar.lua'))()
else
  loadstring(game:HttpGet('https://scripts.luawl.com/11948/Universal.lua'))()
end

